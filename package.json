{
   "name": "cordoffitty",
   "version": "0.0.1",
   "description": "Services management system",
   "productName": "Cordoffitty Mobile",
   "author": "",
   "private": true,
   "scripts": {
      "copy-env:local": "cp ../client/.env-local .env",
      "copy-env:dev": "cp ../client/.env-dev .env",
      "copy-env:prod": "cp ../client/.env-prod .env",
      "copy-assets": "cp -r ../client/src/assets/* ./src/assets/",
      "copy-i18n": "cp -r ../client/src/i18n/* ./src/i18n/",
      "copy-boot": "cp -r ../client/src/boot/* ./src/boot/",
      "copy-lib": "cp -r ../client/src/lib/* ./src/lib/",
      "copy-login": "cp ../client/src/pages/Login.vue ./src/pages/Login.vue",
      "copy-logout": "cp ../client/src/pages/Logout.vue ./src/pages/Logout.vue",
      "copy-components-forms": "cp ../client/src/components/forms/* ./src/components/forms/",
      "copy-components-maps": "cp ../client/src/components/maps/* ./src/components/maps/",
      "merge-client-files": "npm-run-all copy-login copy-logout copy-assets copy-i18n copy-boot copy-lib copy-components-forms copy-components-maps",
      "use:prod": "firebase use prod",
      "build": "quasar build -m capacitor -T ios -i",
      "start:local": "quasar dev -m capacitor -T ios -i",
      "start:local:spa": "quasar dev",
      "serve:local": "npm-run-all copy-env:local start:local",
      "serve:dev": "npm-run-all copy-env:dev start:local",
      "serve:local:spa": "npm-run-all copy-env:local start:local:spa",
      "serve:dev:spa": "npm-run-all copy-env:dev start:local:spa",
      "deploy:prod": "npm-run-all copy-env:prod use:prod build"
   },
   "dependencies": {
      "@capacitor/core": "^3.4.3",
      "@capacitor/geolocation": "^1.3.1",
      "@fawmi/vue-google-maps": "^0.9.67",
      "@googlemaps/js-api-loader": "^1.13.8",
      "@quasar/extras": "^1.14.0",
      "axios": "^0.26.0",
      "core-js": "^3.21.1",
      "dotenv": "^16.0.0",
      "firebase": "^9.6.5",
      "jose": "^4.6.1",
      "logrocket": "^2.2.1",
      "logrocket-vuex": "^0.0.3",
      "quasar": "^2.7.1",
      "vue": "3",
      "vue-i18n": "^9.1.9",
      "vue-logger-plugin": "^2.2.3",
      "vue-router": "4",
      "vuex": "4"
   },
   "devDependencies": {
      "@babel/eslint-parser": "^7.17.0",
      "@quasar/app-webpack": "^3.5.3",
      "eslint": "^8.9.0",
      "eslint-config-prettier": "^8.4.0",
      "eslint-plugin-vue": "^8.4.1",
      "eslint-webpack-plugin": "^3.1.1"
   },
   "browserslist": [
      "last 10 Chrome versions",
      "last 10 Firefox versions",
      "last 4 Edge versions",
      "last 7 Safari versions",
      "last 8 Android versions",
      "last 8 ChromeAndroid versions",
      "last 8 FirefoxAndroid versions",
      "last 10 iOS versions",
      "last 5 Opera versions"
   ],
   "engines": {
      "node": ">= 12.22.1",
      "npm": ">= 6.13.4",
      "yarn": ">= 1.21.1"
   }
}
